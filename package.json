    "esm-path": "1.0.1",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "jest": "29.2.0",
    "jest-environment-jsdom": "29.2.0",
    "jest-environment-node": "29.2.0",
    "microbundle": "0.15.1",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "2.0.10",
    "react-is": "18.2.0",
    "rollup": "2.79.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-sizes": "1.0.4",
