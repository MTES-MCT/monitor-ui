{
  "name": "@mtes-mct/monitor-ui__root",
  "description": "Common React components, hooks, utilities and CSS stylesheets for MonitorFish, MonitorEnv and RapportNav.",
  "version": "24.12.0",
  "license": "AGPL-3.0",
  "private": true,
  "type": "module",
