  "lint-staged": {
    "*.{json,md,yaml,yml}": "prettier --write",
    "*.{ts,tsx}": [
      "eslint",
      "bash -c 'npm run test:type:partial'"
    ]
  },
  "packageManager": "yarn@3.2.4",
  "release": {
    "extends": "@ivangabriele/semantic-release-config-dist"
  }
