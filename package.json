{
  "name": "@mtes-mct/monitor-ui__root",
  "description": "Common React UI components and styles for Monitorfish and Monitorenv.",
  "version": "2.9.4",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": ">=18"
